{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "User",
    "description": "User Test",
    "default": {},
    "required": [
        "email",
        "username",
        "password"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The Id"
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The Users Email, Must Be Unique",
            "examples": [
                "test@test.com"
            ]
        },
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The Users Username",
            "examples": [
                "test"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The Users Hashed Password",
            "examples": [
                "2b$10$0kYoUO.JpNbb.HXnJJExvu9O4u6cFGFWwNkVKnxkJujb4sz/0tLYy"
            ]
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "Creation Date"
        }
    },
    "additionalProperties": false
}
